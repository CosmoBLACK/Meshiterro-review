<div>
  <h3><%= @user.name %></h3>
  <%= image_tag @user.get_profile_image(100, 100) %>
  <!--画面に表示しているユーザーと、ログインしているユーザーのIDが一致するか確認する-->
  <% if @user == current_user %>
    <p><%= link_to "プロフィール編集", edit_user_path(@user) %></p>
  <% end %>
</div>

<%= render 'post_images/list', post_images: @post_images %>